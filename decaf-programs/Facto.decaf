class Facto {
    int facto (int n)
    {
        if (n <= 2)
        {
            return n;
        }
        
        return n * facto(n-1);
    }
    void main ()
    {
        int x;
        WriteString ("Entrer un nombre: ");
        x = ReadInt();
        WriteString ("Factoriel de ");
        WriteInt (x);
        WriteString (" = ");
        WriteInt (facto(x));
        WriteString ("\n");
    }
}
